#include <stdio.h>
int main(void){

int i,j;

for (i = 1; i <= 5; i++)//üst yarı
{
	for (j = 1; j <= i; j++)
	{
		printf("*");
	}
	for (j = 5; j >= i; j--)
	{
		printf(" ");
	}
	for (j = 5; j >= i; j--)
	{
		printf(" ");
	}
	for (j = 1; j <= i; j++)
	{
		printf("*");
	}
	printf("\n");

}

for (i = 1; i <= 5; i++) //alt yarı
{
	for (j = 5; j >= i; j--)
	{
		printf("*");
	}
	for (j = 1; j <= i; j++)
	{
		printf(" ");
	}
	for (j = 1; j <= i; j++)
	{
		printf(" ");
	}
	for (j = 5; j >= i; j--)
	{
		printf("*");
	}
	printf("\n");
}


	return 0;
}
